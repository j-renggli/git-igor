<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ccc" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="/home/jrenggli/prog/ccc" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="debug">
				<Option output="build/debug/bin/ccc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="(cd build/debug; $make -f $makefile ccc)" />
					<CompileFile command="(cd build/debug; $make -f $makefile $file)" />
					<Clean command="(cd build/debug; $make -f $makefile clean)" />
					<DistClean command="(cd build/debug; $make -f $makefile distclean)" />
					<AskRebuildNeeded command="(cd build/debug; $make -q -f $makefile ccc)" />
					<SilentBuild command="(cd build/debug; $make -s -f $makefile ccc)" />
				</MakeCommands>
			</Target>
			<Target title="release">
				<Option output="build/release/bin/ccc" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<MakeCommands>
					<Build command="(cd build/release; $make -f $makefile ccc)" />
					<CompileFile command="(cd build/release; $make -f $makefile $file)" />
					<Clean command="(cd build/release; $make -f $makefile clean)" />
					<DistClean command="(cd build/release; $make -f $makefile distclean)" />
					<AskRebuildNeeded command="(cd build/release; $make -q -f $makefile ccc)" />
					<SilentBuild command="(cd build/release; $make -s -f $makefile ccc)" />
				</MakeCommands>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
		</Compiler>
		<Extensions>
			<code_completion />
			<debugger />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
